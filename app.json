{
  "expo": {
    "name": "InnovaTech Product Scanner",
    "slug": "InnovaTech-Product-Scanner",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/appIcon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/appSplash.png",
      "resizeMode": "cover",
      "backgroundColor": "#365b44"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to take photos and videos.",
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to save photos and videos."
      }
    },
    "android": {
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "RECORD_AUDIO"
      ],
      "package": "com.saiah.InnovaTechProductScanner"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "8c068461-3502-4749-bc72-96cf02ab63be"
      }
    }
  }
}
